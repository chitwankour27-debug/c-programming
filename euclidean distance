#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    #include <math.h>
    int x1, y1, x2, y2;
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);
    //Calculate Euclidean distance
    double distance = sqrt ((x2-x1) * (x2-x1) + (y2-y1) * (y2-y1));
    
    //Print distance with 2 decimal places
    printf("%.2f\n", distance);
    
    

    
    return 0;
}

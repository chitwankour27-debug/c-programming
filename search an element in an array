#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, i;
    scanf("%d", &n);
    int ar[n];
    int a;for(i=0;i<n; i++){
        scanf("%d" ,&ar[i]);
    }
    scanf("%d", &a);
    
    int flag=0;
    for (i=0;i<n;i++){
        if (ar[i]==a){
            flag=1;
            break;
            
        }
    }
    if (flag==1){
        
        printf("Element found at position %d", i+1);
    }
    else{
        printf("Element not found");
    }

    
    return 0;
}

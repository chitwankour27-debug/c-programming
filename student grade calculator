#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int Physics, Chemistry, Biology, Mathematics, Computer;
    scanf("%d %d %d %d %d", &Physics, &Chemistry, &Biology, &Mathematics, &Computer);
    float Percentage;
    Percentage = (float)(Physics + Chemistry + Biology + Mathematics + Computer) / 500.00 * 100;
    printf("Percentage = %.2f\n",Percentage);

    if (Percentage >= 90)
        printf("Grade = A\n");
    else if (Percentage >= 80)
        printf("Grade = B\n");
    else if (Percentage >= 70)
        printf("Grade = C\n");
    else if (Percentage >= 60)
        printf("Grade = D\n");
    else if (Percentage >= 40)
        printf("Grade = E\n");
    else
        printf("Grade = F\n");

    
    return 0;
}
